{
  "name": "tailwindcss-boilerplate",
  "version": "1.0.0",
  "description": "A simple Tailwind CSS boilerplate",
  "scripts": {
    "dev": "tailwindcss build tailwind.css -o public/css/tailwind.output.css",
    "build": "cross-env NODE_ENV=production postcss tailwind.css -o public/css/tailwind.output.css"
  },
  "keywords": [
    "tailwind",
    "boilerplate",
    "simple",
    "html",
    "css",
    "minify",
    "minification"
  ],
  "author": "Estevan Maito <ejmaito@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "9.8.4",
    "cssnano": "4.1.10",
    "postcss-cli": "7.1.1",
    "tailwindcss": "1.4.6"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "cross-env": "7.0.2"
  }
}
